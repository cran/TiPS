Package: TiPS
Type: Package
Title: Trajectories and Phylogenies Simulator
Version: 1.2.1
Date: 2022-08-26
Authors@R: c(person("Gonche", "Danesh", role = c("aut", "cre", "cph"), email = "gonche.danesh@gmail.com"),
	     person("Emma", "Saulnier", role = c("aut", "cph")),
			 person("Olivier", "Gascuel", role = c("aut", "cph")),
